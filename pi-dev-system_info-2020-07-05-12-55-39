===============================================================================
 CONTENTS OF RC.LOCAL
===============================================================================

_IP=$(hostname -I) || true
if [ "$_IP" ]; then
  printf "My IP address is %s\n" "$_IP"
fi
/usr/local/scripts/my_boottasks
exit 0

